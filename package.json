    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.55",
    "@types/zen-observable": "^0.5.3",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
