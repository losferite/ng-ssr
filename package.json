  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --port 8000",
    "serve:dynamic": "node --experimental-modules dist/server.js",
    "start": "npm run ssr",
    "ssr": "npm run build:dynamic && npm run serve:dynamic",
    "build:dynamic": "npm run build:client && npm run build:server && npm run webpack:server",
    "build:client": "ng build --prod",
    "build:server": "ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js -p --progress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.3",
